@using AL_LAB2.Models
@model  StudentEnrollmentVM

@for (int i = 0; i < Model.coursesEnroll.Count; i++)
{
    @Html.HiddenFor(model => model.coursesEnroll[i].Id)
    <dt>
        @Html.EditorFor(model => model.coursesEnroll[i].isEnroll)
        @Html.DisplayFor(model => model.coursesEnroll[i].CourseCode)
    </dt>
    <dd>
        @Html.DisplayFor(model => model.coursesEnroll[i].CourseName)
    </dd>
}
